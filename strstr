		`
	}
}
int main()
{
	//strstr（p1,p2）在p1中找有没有p2，如果有返回p1中p2[0]字符的下标，如果没有，返回一个(null)；
	const char* p1 = "abcdef";
	const char* p2 = "defd";
	//printf("%s\n", strstr(p1, p2));
	printf("%s\n", my_strstr(p1, p2));
